class arun {
    public static void main(String[] args) {
        System.out.println("Enter the row count");
        Scanner sc = new Scanner(System.in);
        int rows = sc.nextByte();
        for (int i = 1; i <= rows; ++i) {
            for (int j = 1; j <= i; ++j) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
