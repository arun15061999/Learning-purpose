package com.arun;

class arun {
    public static void main(String[] args) {
        char ch = 'a';
        if ((ch >= 'a' && ch <= 'z')||(ch >= 'A' && ch <= 'Z'))
        {
            System.out.println("The given num is char");
        }
    else
        {
            System.out.println("The given num is not char");
        }
    }
}
