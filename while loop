package com.arun;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("enter the number");
        int a = scanner.nextInt();
        while(a<10){
            System.out.println(" the number is " +a);
            ++a;
        }
    }
