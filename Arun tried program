package com.arun;

import java.io.IOException;
import java.util.Scanner;

public class marks {
    void scoring() throws IOException {
        System.out.println("Enter the total mark");
        Scanner mark = new Scanner(System.in);
        int cout = mark.nextInt();
        int g;
        for (int x = 0; x < cout; x++) {
            System.out.println("Enter the subject score");
            g = (int) System.in.read();
            break;
