package com.arun;

import java.io.IOException;
import java.util.Scanner;

class students {
    void name() {
        String str;
        System.out.println("Name of the students");
        Scanner studentname = new Scanner(System.in);
        String sc = studentname.nextLine();

    }
    void registerid() {
        System.out.println("Enter the total number of students:");
        Scanner id = new Scanner(System.in);
        int count = id.nextInt();
        for (int i = 1; i <= count; i++) {
            System.out.println("Total number of students:   " + i);
        }
    }

    void subjects() throws IOException {
        System.out.println("Enter the total number of subjects:");
        Scanner id = new Scanner(System.in);
        int cont = id.nextInt();
        System.out.println("Total number of subjects:   " + cont);

